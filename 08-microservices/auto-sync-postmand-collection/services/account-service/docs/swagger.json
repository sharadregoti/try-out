                },
                "country_flag": {
                    "type": "string"
                },
                "created_at": {
                    "type": "string"
                },
                "currency_symbol": {
                    "type": "string"
                },
                "id": {
                    "type": "string"
                },
                "kyc": {
                    "type": "boolean"
                },
                "name": {
                    "type": "string"
                },
                "number": {
                    "type": "string"
                },
                "provider_id": {
                    "type": "string"
                },
                "refreshed_at": {
                    "type": "string"
                },
                "service_status": {
                    "type": "integer"
                },
                "symbol_left": {
                    "type": "boolean"
                },
                "symbol_right": {
                    "type": "boolean"
                },
                "type": {
                    "type": "string"
                },
                "user_id": {
                    "type": "string"
                }
            }
